<p id="notice"><%= notice %></p>

<h1>Businesses</h1>

<table>
  <thead>
    <tr>
      <th>Orgtype</th>
      <th>Name</th>
      <th>Type</th>
      <th>Addline1</th>
      <th>Addline2</th>
      <th>Addcity</th>
      <th>Addstate</th>
      <th>Addzip</th>
      <th>Phone</th>
      <th>Website</th>
      <th>Email1</th>
      <th>Email2</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @businesses.each do |business| %>
      <tr>
        <td><%= business.orgType %></td>
        <td><%= business.name %></td>
        <td><%= business.type %></td>
        <td><%= business.addLine1 %></td>
        <td><%= business.addLine2 %></td>
        <td><%= business.addCity %></td>
        <td><%= business.addState %></td>
        <td><%= business.addZip %></td>
        <td><%= business.phone %></td>
        <td><%= business.website %></td>
        <td><%= business.email1 %></td>
        <td><%= business.email2 %></td>
        <td><%= link_to 'Show', business %></td>
        <td><%= link_to 'Edit', edit_business_path(business) %></td>
        <td><%= link_to 'Destroy', business, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Business', new_business_path %>
